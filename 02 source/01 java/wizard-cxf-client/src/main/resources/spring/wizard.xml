<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<bean id="login" class="com.wizard.cxf.application.soa.system.ILogin"
		factory-bean="loginFactory" factory-method="create" />
	<bean id="loginFactory" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">
		<property name="serviceClass" value="com.wizard.cxf.application.soa.system.ILogin" />
		<property name="address" value="http://localhost:8080/wizard-cxf/login" />
	</bean>

</beans>